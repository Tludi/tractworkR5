<p id="notice"><%= notice %></p>

<h1>Admin Csi Divisions</h1>

<table>
  <thead>
    <tr>
      <th>Divnum</th>
      <th>Divtitle</th>
      <th>Groupname</th>
      <th>Subgroupname</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @admin_csi_divisions.each do |admin_csi_division| %>
      <tr>
        <td><%= admin_csi_division.divnum %></td>
        <td><%= admin_csi_division.divtitle %></td>
        <td><%= admin_csi_division.groupname %></td>
        <td><%= admin_csi_division.subgroupname %></td>
        <td><%= link_to 'Show', admin_csi_division %></td>
        <td><%= link_to 'Edit', edit_admin_csi_division_path(admin_csi_division) %></td>
        <td><%= link_to 'Destroy', admin_csi_division, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Admin Csi Division', new_admin_csi_division_path %>
