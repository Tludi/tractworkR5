 
<div class="md-skin">
  <div class="row wrapper border-bottom white-bg page-heading">
    <div class="col-lg-10">
      <h2><%= @account.name %>'s Materials</h2>
      <ol class="breadcrumb">
        <li>
          <%= link_to 'Dashboard', admin_root_path  %>
        </li>
        <li class="active">
          <strong>Users</strong>
        </li>
      </ol>
    </div>
    <div class="col-lg-2">

    </div>

  </div>

  <p id="notice"><%= notice %></p>

  <div class="wrapper wrapper-content animated fadeInRight">
  <div class="row">
    <div class="col-lg-12">
      <div class="ibox float-e-margins">
        <div class="ibox-title">
          <h5><%= @account.name %> Materials</h5>
          <div class="ibox-tools">
            <a class="collapse-link">
              <i class="fa fa-chevron-up"></i>
            </a>
            <a class="dropdown-toggle" data-toggle="dropdown" href="#">
              <i class="fa fa-wrench"></i>
            </a>
            <ul class="dropdown-menu dropdown-user">
              <li>
                <a href="#">Config option 1</a>
              </li>
              <li>
                <a href="#">Config option 2</a>
              </li>
            </ul>
            <a class="close-link">
              <i class="fa fa-times"></i>
            </a>
          </div>
        </div>
        <div class="ibox-content">


          <h1>Materials</h1>

          <table class="table table-striped table-bordered table-hover projects display" cellspacing="0" width="100%">
            <thead>
              <tr>
                <th>Division</th>
                <th>Title</th>
                <th>Description</th>
                <th>Measure</th>
                <th>Cost</th>
                <th colspan="3"></th>
              </tr>
            </thead>

            <tbody>
              <% @materials.each do |material| %>
                <tr>
                  <td><%= material.divnum %></td>
                  <td><%= material.divname %></td>
                  <td><%= material.description %></td>
                  <td><%= material.defmeasure %></td>
                  <td><%= material.cost %></td>
                  <td><%= link_to 'Show', material %></td>
                  <td><%= link_to 'Edit', edit_material_path(material) %></td>
                  <td><%= link_to 'Destroy', material, method: :delete, data: { confirm: 'Are you sure?' } %></td>
                </tr>
              <% end %>
            </tbody>
          </table>
          <%= link_to 'New Material', new_material_path %>
          </div>
        </div>
      </div>
    </div>
  </div>
</div>
<br>

