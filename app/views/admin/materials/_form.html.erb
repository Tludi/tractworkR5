<div class="md-skin">
<div class="row wrapper border-bottom white-bg page-heading">
  <div class="col-lg-10">
    <ol class="breadcrumb">
      <li>
        <%= link_to 'Dashboard', admin_root_path  %>
      </li>
      <li>
        <%= link_to 'Materials', admin_materials_path  %>
      </li>
      <li class="active">
        <strong>Form</strong>
      </li>
    </ol>
  </div>
  <div class="col-lg-2">

  </div>
</div>
<div class="wrapper wrapper-content animated fadeInRight">
  <div class="row">
    <div class="col-lg-12">
      <div class="ibox float-e-margins">
        <div class="ibox-title">
          <h5>Add/Edit Project Form</h5>
          <div class="ibox-tools">
            <a class="collapse-link">
              <i class="fa fa-chevron-up"></i>
            </a>
          </div>
        </div>
        <div class="ibox-content center">
          <%= simple_form_for([:admin,@material]) do |f| %>
            <%= f.error_notification %>

            <div class="form-inputs">
              <%= f.input :divisionnum, label: "Division Number"%>
              <%= f.input :divisionname, label: "Division Name" %>
              <%= f.input :itemdescription, label: "Item Description" %>
              <%= f.input :defmeasure, label: "Default units" %>
              <%= f.input :unitcost, label: "Unit cost" %>
              <%= f.input :itemwaste, label: "Waste %" %>
            </div>

            <div class="form-actions">
              <%= f.button :submit %>
            </div>
          <% end %>
        </div>
      </div>
    </div>
  </div>
</div>
</div>