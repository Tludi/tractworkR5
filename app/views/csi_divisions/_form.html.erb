<%= simple_form_for(@csi_division) do |f| %>
  <%= f.error_notification %>

  <div class="form-inputs">
    <%= f.input :divnum %>
    <%= f.input :divtitle %>
    <%= f.input :groupname %>
    <%= f.input :subgroupname %>
  </div>

  <div class="form-actions">
    <%= f.button :submit %>
  </div>
<% end %>
